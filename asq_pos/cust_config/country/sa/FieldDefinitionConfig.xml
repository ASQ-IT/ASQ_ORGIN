<?xml version="1.0" encoding="UTF-8"?>
<FieldSet xmlns="http://micros.com/xstore/config/form/field" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormFieldConfig.xsd">
 
   <!-- ASQ Zatca Integration Starts -->
   <Field name="captureOTP" type="Text" resource="captureOTP" dataFieldRef="ASQ_OTP"/>
   <Field name="captureOTPLabel" type="Label" text="_captureOTP" />
   <!-- ASQ Zatca Integration Ends -->
   
   <!-- ASQ STC Integration Starts -->
   <Field name="stcOTP" type="Text" resource="stcOTP" dataFieldRef="ASQ_OTP"/>
   <Field name="stcOTPLabel" type="Label" text="_asqPromptmsgEnterOTP" />
   
   <Field name="stcRedeemPoints" type="Text" resource="stcRedeemPoints" dataFieldRef="Numeric"/>
   <Field name="stcRedeemPointsLabel" type="Label" text="_asqPromptmsgEnterRedeemPoints" />
  <!-- ASQ STC Integration Ends -->
  <!-- ASQ Neqaty Integration Starts -->
   <Field name="neqatyOTP" type="Text" resource="neqatyOTP" dataFieldRef="ASQ_OTP"/>
   <Field name="neqatyOTPLabel" type="Label" text="_asqPromptmsgEnterOTP" />
   
   <Field name="neqatyRedeemPoints" type="Text" resource="neqatyRedeemPoints" dataFieldRef="Numeric"/>
   <Field name="neqatyRedeemPointsLabel" type="Label" text="_asqPromptmsgEnterRedeemPoints" />
  <!-- ASQ Neqaty Integration Ends -->
   <!-- ASQ Capture Customer Mobile Number Starts -->
   <Field name="custMobileNumber" type="Text" resource="custMobileNumber" dataFieldRef="Phone"/>
   <Field name="custMobileNumberLabel" type="Label" text="_asqPromptmsgEnterMobileNumber" />
   <!-- ASQ Capture Customer Mobile Number Ends -->
   
  </FieldSet>
  