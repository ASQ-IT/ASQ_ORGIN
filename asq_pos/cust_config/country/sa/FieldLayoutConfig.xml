<?xml version="1.0" encoding="UTF-8"?>
<FieldLayoutSet xmlns="http://micros.com/xstore/config/form/fieldlayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FieldLayoutConfig.xsd">
   
   <!-- ASQ Zatca Integration Starts -->
   <FieldLayout name="captureOTPLayout" type="SimpleLayout">
    	<Row>
      		<Column start="0" width="100" fieldRef="captureOTP" required="true" labeledBy="captureOTPLabel" verticalAlignment="Center"/>
    	</Row>
   </FieldLayout>
   <!-- ASQ Zatca Integration Ends -->
   
   <!-- ASQ STC Integration Starts -->
   <FieldLayout name="stcOTPLayout" type="SimpleLayout">
    <Row>
			<Column start="0" width="100" fieldRef="stcOTP" required="true" refreshOnChange="true" labeledBy="stcOTPLabel" verticalAlignment="Center" />
    </Row>
     <Row>
			<Column start="0" width="100" fieldRef="stcRedeemPoints" required="true" refreshOnChange="true" labeledBy="stcRedeemPointsLabel" verticalAlignment="Center" />
    </Row>
   </FieldLayout>
   <!-- ASQ STC Integration Ends -->
   
       <!-- ASQ Neqaty Integration Starts -->
   <FieldLayout name="neqatyOTPLayout" type="SimpleLayout">
    <Row>
      <Column start="0" width="100" fieldRef="neqatyOTP" required="true" labeledBy="neqatyOTPLabel" verticalAlignment="Center"/>
    </Row>
   </FieldLayout>
   <!-- ASQ Capture Customer Mobile Number Starts -->
   <FieldLayout name="ASQ_MOBILE_NUMBER_LAYOUT" type="SimpleLayout">
	   	<Row>
			<Column start="0" width="100" fieldRef="custMobileNumber" required="true" refreshOnChange="true" labeledBy="custMobileNumberLabel" verticalAlignment="Center" />
	   	</Row>
   </FieldLayout>
   <!-- ASQ Capture Customer Mobile Number Ends -->

   
   <!--  ASQ PLanet Tax Integeration -->
  <FieldLayout name="asqPlanetDetailsLayout" type="ListLayout">
    <Row>
      <Column start="0" width="100" fieldRef="asqCustInfoLabel" accessibilityText="_commonCustInfo"/>
    </Row>
    <Row>
      <Column start="10" width="35" fieldRef="firstName" required="true" labeledBy="firstNameLabel" />
      <Column start="55" width="30" fieldRef="lastName"  labeledBy="lastNameLabel" />
    </Row>
    <Row>
      <Column start="10" width="35" fieldRef="custMobileNumber" refreshOnChange="true" required="true" labeledBy="custMobileNumberLabel" />
      <Column start="55" width="30" fieldRef="asqGender" refreshOnChange="true" required="true" labeledBy="genderLabel" />
    </Row>
    <Row>
     <Column start="10" width="75" fieldRef="address1" refreshOnChange="true" labeledBy="address1Label" />
    </Row>
     <Row>
      <Column start="10" width="35" fieldRef="asqNationality" refreshOnChange="true" required="true" labeledBy="asqNationalityLabel" />
       <Column start="55" width="30" fieldRef="asqNationalityResidence" refreshOnChange="true" required="true" labeledBy="asqNationalityResidenceLabel" />
    </Row>
  </FieldLayout>
   <FieldLayout name="asqPlanetTaxDetailsLayout" type="ListLayout">
    <Row>
      <Column start="0" width="100" fieldRef="asqCustInfoTaxLabel" accessibilityText="_asqCustInfoTaxLabel"/>
    </Row>
     <Row>
      <Column start="10" width="35" fieldRef="asqTaxDocument" required="true" labeledBy="asqTaxDocumentLabel"/>
      <Column start="55" width="30" fieldRef="asqTaxDocumentNumber" required="true" labeledBy="asqTaxDocumentNumberLabel"/>
    </Row>
     <Row>
      <Column start="10" width="35" fieldRef="asqTaxIssuedBy" required="true" labeledBy="asqTaxIssuedByLabel"/>
      <Column start="55" width="30" fieldRef="asqExpirationDate" labeledBy="asqExpirationDateLabel"/>
    </Row>
    <Row>
      <Column start="10" width="35" fieldRef="asqBirthDate" required="true" labeledBy="asqbirthDateLabel"/>
    </Row>
  </FieldLayout>
  
     <!-- ASQ Mokafaa Integration Starts -->
   
    <FieldLayout name="mokafaaLoyaltyLayout" type="SimpleLayout">
    	<Row>
	      	<Column start="0" width="100" fieldRef="custMobileNumber" labeledBy="custMobileNumberLabel" verticalAlignment="Center"/>
	   	</Row>
	   	
	    <Row>
	      <Column start="0" width="100" fieldRef="loyaltyCICNo" labeledBy="mokafaaloyaltyCICNoLabel" verticalAlignment="Center"/>
	    </Row>
   </FieldLayout>
   
   <FieldLayout name="mokafaaOTPLayout" type="SimpleLayout">
    <Row>
      <Column start="0" width="100" fieldRef="mokafaaOTP" required="true" labeledBy="mokafaaLabel" verticalAlignment="Center"/>
    </Row>
     <Row>
      <Column start="0" width="100" fieldRef="mokafaaRedeemPoints" required="true" labeledBy="mokafaaRedeemPointsLabel" verticalAlignment="Center"/>
    </Row>
   </FieldLayout>
   <!-- ASQ Mokafaa Integration Ends -->
	<!-- ASQ STC Customer Maintenance Changes - Starts-->
	<FieldLayout name="customerNameLayout" type="ListLayout">
    <Row>
      <Column start="0" width="25" fieldRef="firstName" refreshOnChange="true" labeledBy="firstNameShortLabel" />
      <Column start="25" width="10" fieldRef="middleName" refreshOnChange="true" labeledBy="middleNameLabel" />
      <Column start="35" width="25" fieldRef="lastName" refreshOnChange="true" labeledBy="lastNameLabel" />
      <Column start="70" width="30" fieldRef="organizationName" refreshOnChange="true" fieldSubmitPriority="2" labeledBy="organizationNameLabel" />
    </Row>
  </FieldLayout>
  
	<FieldLayout name="custContactInformationLayout" type="ListLayout">
   <Row>      
      <Column start="0" width="35" fieldRef="customerContactInformationLabel" fontRef="formContentHeader" accessibilityText="_customerContactInformation"/>
      <Column start="37" width="13" fieldRef="okToContact" horizontalAlignment="Center"/>
      <Column start="87" width="13" fieldRef="okToContact" horizontalAlignment="Center"/>
    </Row>
    <Row>
      <Column start="0" width="35" fieldRef="telephone1" refreshOnChange="true" labeledBy="telephone1Label" />
      <Column start="37" width="13" fieldRef="telephone1Contact" horizontalAlignment="Center" refreshOnChange="true" />
      <Column start="50" width="35" fieldRef="telephone2" refreshOnChange="true" labeledBy="telephone2Label" />
      <Column start="87" width="13" fieldRef="telephone2Contact" horizontalAlignment="Center" refreshOnChange="true" />
    </Row>
    <Row>
      <Column start="0" width="35" fieldRef="telephone3" refreshOnChange="true"  required="true" labeledBy="telephone3Label" />
      <Column start="37" width="13" fieldRef="telephone3Contact" horizontalAlignment="Center" refreshOnChange="true" />
      <Column start="50" width="35" fieldRef="emailAddress" refreshOnChange="true" labeledBy="emailAddressLabel" />
      <Column start="87" width="13" fieldRef="emailContact" horizontalAlignment="Center" refreshOnChange="true" />
    </Row>
    <Row>
      <Column start="50" width="35" fieldRef="emailRcptsLabel" horizontalAlignment="Right" />
      <Column start="87" width="13" fieldRef="emailRcpts" horizontalAlignment="Center" refreshOnChange="true" />
    </Row>
  </FieldLayout>
  <!-- ASQ STC Customer Maintenance Changes - Ends-->
  
  <!-- ASQ STC Customer Search Changes - Starts-->
  <FieldLayout name="customerSearchLayout" type="SimpleLayout">
    <Row>
      <Column fieldRef="telephoneSearch" accessibilityText="_formPhoneNumberLabel" required="true" labeledBy="telephoneLabel" />
    </Row>
    <Row>
      <Column fieldRef="asqlastName" labeledBy="lastNameLabel" />
    </Row>
    <Row>
      <Column fieldRef="firstName" labeledBy="firstNameLabel" />
    </Row>
    <Row>
      <Column fieldRef="city" labeledBy="cityLabel" />
    </Row>
    <Row>
      <Column fieldRef="state" labeledBy="stateLabel" />
    </Row>
    <Row>   
      <Column />   
      <Column fieldRef="country" horizontalAlignment="Right" />      
    </Row>
    <Row>
      <Column fieldRef="postalCode" refreshOnChange="true" labeledBy="postalCodeLabel" />
    </Row>
    <Row>
      <Column fieldRef="customerIdInput" labeledBy="customerIdInputLabel" />
    </Row>
    <Row>
      <Column fieldRef="county" visibilityGroup="countyVisibility" labeledBy="countyLabel" />
    </Row>
  </FieldLayout>
  <!-- ASQ STC Customer Search Changes - Ends-->
</FieldLayoutSet>
